{
    "rules": {
        "node/no-unsupported-features/es-syntax": "off",
        "node/no-missing-import": "off"
    },
    "overrides": [{
        "files": "*.md{js}",
        "rules": {
            "prefer-arrow-callback": "off",
            "no-var": "off"
        }
    }],
    "extends": [
        "plugin:node/recommended",
        "plugin:putout/recommended"
    ],
    "plugins": [
        "node",
        "putout"
    ]
}
